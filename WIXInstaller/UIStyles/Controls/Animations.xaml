<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Storyboard x:Key="SocialsClick">
        <DoubleAnimation Storyboard.TargetName="MyImg" Storyboard.TargetProperty="(Canvas.Top)" To="500" Duration="0:0:0.9" AutoReverse="True" />
        <DoubleAnimation Storyboard.TargetName="MyImg" Storyboard.TargetProperty="(Canvas.Left)" To="850" Duration="0:0:0.7" AutoReverse="True" />
    </Storyboard>

</ResourceDictionary>